<%
/* creates a link to an attribute in an interface */
/* parameters: interface_name, attribute_name */

var lang = env.locale;

var destNewType = "/" + lang + '/docs/XPCOM_Interface_Reference/' + $0;
var apiString = $0 + '.' + $1;
var html = "";

html = web.link(wiki.uri(destNewType) + '#' + $1, apiString);

%>
<code><%-html%></code>
